<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Paquete para realizar operaciones en segundo plano con xamarin forms</Description>
    <Version>1.1.1</Version>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Authors>Arsoftcr</Authors>
    <Company>Arsoftcr</Company>
    <Product>Xamarin Forms</Product>
    <PackageProjectUrl>https://github.com/arsoftcr/BackgroundXamarinForms</PackageProjectUrl>
    <RepositoryUrl>https://github.com/arsoftcr/BackgroundXamarinForms</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Xamarin.Forms" Version="4.2.0.910310" />
  </ItemGroup>

</Project>
